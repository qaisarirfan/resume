    "start": "cross-env REACT_APP_ENV=dev REACT_APP_REDUX_LOGER=false react-scripts start",
    "start:redux_loger": "cross-env REACT_APP_ENV=dev REACT_APP_REDUX_LOGER=true react-scripts start",
